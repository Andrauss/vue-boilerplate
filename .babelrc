{
  "presets": [
    "es2015",
    "es2017",
    "stage-0",
    [
      "env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "add-module-exports"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "vue-app"
      ],
      "plugins": [
        "istanbul"
      ]
    }
  }
}
