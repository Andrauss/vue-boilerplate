{
  "name": "vue-boilerplate",
  "description": "An opinionated Vue.js 2 boilerplate",
  "version": "1.0.0",
  "author": "Marcos Moura <marcosvmmoura@gmail.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/marcosmoura/vue-boilerplate"
  },
  "scripts": {
    "dev": "babel-node build/server/development.js --presets es2015,stage-0",
    "build": "rm -rf dist && babel-node build/server/production.js --presets es2015,stage-0",
    "test": "ava **/*.test.js",
    "coverage": "rm -rf .nyc_output coverage && mkdir .nyc_output && cross-env NODE_ENV=test nyc --reporter=html --reporter=text yarn run test",
    "lint": "eslint --ext .js,.vue --fix src"
  },
  "dependencies": {
    "vue": "^2.2.5",
    "vue-router": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "autosize": "^3.0.20",
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-vue-app": "^0.5.1",
    "browser-env": "^2.0.25",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "clipboard": "^1.6.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "css-mqpacker": "^5.0.1",
    "eslint": "^3.18.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-html": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.1",
    "nyc": "^10.1.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "optimize-js-plugin": "^0.0.4",
    "ora": "^1.2.0",
    "sass-loader": "^6.0.3",
    "url-loader": "^0.5.8",
    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.3.3",
    "vue-node": "^1.0.2",
    "vue-style-loader": "^2.0.4",
    "vue-template-compiler": "^2.2.5",
    "webpack": "^2.3.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0"
  },
  "browserslist": [
    "last 3 versions",
    "not IE < 10"
  ],
  "nyc": {
    "cache": false,
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "build",
      "**/*.test.js"
    ],
    "extension": [
      ".js",
      ".vue"
    ],
    "require": [
      "babel-register"
    ]
  },
  "ava": {
    "require": [
      "babel-register",
      "./build/test/setup.js"
    ]
  }
}
